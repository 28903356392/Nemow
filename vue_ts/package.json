{
  "name": "vue_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && vue-tsc && vite build ",
    "preview": "vite preview",
    "lint": "npm run lint:es && npm run lint:pr && npm run lint:st",
    "lint:es": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
    "lint:pr": "prettier --write \"./**/*.{html,vue,js,ts,json,md}\" ",
    "lint:st": "stylelint  src/**/*.{vue,scss,css,sass,less} --fix",
    "prepare": "cd .. && cross-var husky install $npm_package_name/.husky"
  },
  "dependencies": {
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.1",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.5"
  }
}
